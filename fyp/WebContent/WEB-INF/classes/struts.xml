<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	<package name="fyp" extends="struts-default, json-default">
		<action name="databaseConnection" class="fyp.DatabaseConnectionAction"
			method="execute">
			<result name="Seriouserror">/EEK/pages-500.html</result>
			<result name="error">/EEK/pages-500.html</result>
			<result type="json">
				<param name="root">dataMap</param>
			</result>
		</action>
	</package>
</struts>